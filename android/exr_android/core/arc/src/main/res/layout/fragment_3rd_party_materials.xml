                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_marginLeft="32dp"
                android:layout_marginRight="32dp"
                android:layout_weight="1"
                android:lineSpacingExtra="3dp"
                android:paddingTop="25dp"
                android:text="The ARC open source code and all open source 3rd Parties used by the ARC open source code in accordance with the ARC User Agreement and as posted in GitHub. "
                android:textColor="@color/white"
                android:textSize="18sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textViewSubHeader2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="32dp"
                android:layout_marginRight="32dp"
                android:layout_weight="1"
                android:autoLink="web"
                android:lineSpacingExtra="3dp"
                android:linksClickable="true"
                android:paddingTop="16dp"
                android:paddingBottom="0dp"
                android:text="The ARC Open Source Code can be found at: https://github.com/jasonhass/Ambulatory-Research-in-Cognition"
